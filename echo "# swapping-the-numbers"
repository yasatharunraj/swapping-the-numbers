#include <stdio.h>

int main()
{
  int a,b;
  printf("assign an integer value to \'a\'\n");
  scanf("%d",&a);
  printf("assign an integer value to \'b\'\n");
  scanf("%d",&b);
  printf("Now, The values of a and b are %d and %d respectively\n",a,b);
  a=a+b;
  b=a-b;
  a=a-b;
  printf("And after swapping the values, The values of a and b are %d and %d respectively\n",a,b);
}
